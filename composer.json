{
	"name": "stefna/cookie",
	"description": "Cookie management for psr7/psr15",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Sundqvist",
			"email": "andreas@stefna.is"
		}
	],
	"require": {
		"php": "^8.2",
		"psr/clock": "^1.0",
		"psr/log": "^3.0",
		"psr/http-message": "^1.0.1",
		"psr/http-server-middleware": "^1.0.1",
		"stefna/ds-collection": "dev-main"
	},
	"require-dev": {
		"sunkan/dictus": "^2.0",
		"nyholm/psr7": "^1.0",
		"squizlabs/php_codesniffer": "dev-master",
		"stefna/codestyle": "^1.12",
		"phpstan/phpstan": "^1.8",
		"bnf/phpstan-psr-container": "^1.0",
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan-phpunit": "^1.1",
		"tomasvotruba/type-coverage": "^0.0.7",
		"phpstan/extension-installer": "^1.2"
	},
	"autoload": {
		"psr-4": {
			"Stefna\\Cookie\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stefna\\Cookie\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
